    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dev-task",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "cpu": 256,
      "memoryReservation": 128,
      "image": "${image}",
      "name": "dev-container"
    }
  ]